doctype html
html
  head
    meta charset="utf-8"
    meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1"

    title Ruby on Rails Web Development | Insignia
    link href="assets/insignia_logo_small.ico" rel="shortcut icon"
    link href="assets/iconos/insignia_ico_57.ico" rel="apple-touch-icon-precomposed" sizes="57x57"
    link href="assets/iconos/insignia_ico_114.ico" rel="apple-touch-icon-precomposed" sizes="114x114"
    link href="assets/iconos/insignia_ico_72.ico" rel="apple-touch-icon-precomposed" sizes="72x72"
    link href="assets/iconos/insignia_ico_144.ico" rel="apple-touch-icon-precomposed" sizes="144x144"

    meta name="description" content="Insignia web"
    meta name="viewport" content="width=device-width"

    = stylesheet_link_tag "application", :media => "all"
    = yield :stylesheets
    = javascript_include_tag "application"
    = csrf_meta_tags

  body onload="window.scrollTo(0,1);"
    = render "shared/header"
    .content
      = yield
    = render "shared/footer"

    javascript:
      var _gaq=[['_setAccount','UA-1828552-1'],['_trackPageview']];
      (function(d,t){var g=d.createElement(t),s=d.getElementsByTagName(t)[0];
      g.src=('https:'==location.protocol?'//ssl':'//www')+'.google-analytics.com/ga.js';
      s.parentNode.insertBefore(g,s)}(document,'script'));
